{
  "name": "react-svg-injector",
  "version": "0.0.0-development",
  "description":
    "A React component that manipulate SVG and injects it using SVGInjector",
  "main": "lib/index.js",
  "files": ["lib"],
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "babel ./src -d ./lib",
    "test": "jest",
    "semantic-release": "semantic-release"
  },
  "keywords": ["react", "svg"],
  "author":
    "Aaron A. Reisman <aaron@lifeiscontent.net> (http://aaronreisman.com/)",
  "license": "MIT",
  "resolutions": {
    "istanbul": "^0.4.5",
    "istanbul-api": "1.2.2",
    "istanbul-reports": "1.1.4"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.47",
    "@babel/core": "7.0.0-beta.47",
    "@babel/preset-env": "7.0.0-beta.47",
    "@babel/preset-react": "7.0.0-beta.47",
    "@babel/preset-stage-0": "7.0.0-beta.49",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "22.4.3",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "jest": "22.4.3",
    "prop-types": "15.6.1",
    "raf": "3.4.0",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-test-renderer": "16.3.2",
    "regenerator-runtime": "0.11.1",
    "semantic-release": "15.4.1",
    "sinon": "5.0.7",
    "xhr-mock": "2.3.2"
  },
  "peerDependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "dependencies": {
    "svg-injector": "1.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lifeiscontent/react-svg-injector.git"
  },
  "bugs": {
    "url": "https://github.com/lifeiscontent/react-svg-injector/issues"
  },
  "homepage": "https://github.com/lifeiscontent/react-svg-injector#readme",
  "jest": {
    "collectCoverage": true,
    "testPathIgnorePatterns": ["/node_modules/", "/example/"],
    "setupFiles": ["raf/polyfill", "./jestSetup"],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
