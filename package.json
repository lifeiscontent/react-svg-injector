{
  "name": "react-svg-injector",
  "version": "0.0.0-development",
  "description": "A React component that manipulate SVG and injects it using SVGInjector",
  "main": "dist/index.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "babel lib -d dist",
    "test": "jest",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "keywords": [
    "react",
    "svg"
  ],
  "author": "Aaron A. Reisman <aaron@lifeiscontent.net> (http://aaronreisman.com/)",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.42",
    "@babel/core": "7.0.0-beta.42",
    "@babel/preset-env": "7.0.0-beta.42",
    "@babel/preset-react": "7.0.0-beta.42",
    "@babel/preset-stage-0": "7.0.0-beta.42",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "22.4.3",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "jest": "22.4.3",
    "prop-types": "15.6.1",
    "raf": "3.4.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-test-renderer": "16.2.0",
    "regenerator-runtime": "0.11.1",
    "semantic-release": "^15.1.4",
    "sinon": "4.4.8",
    "xhr-mock": "2.3.0"
  },
  "peerDependencies": {
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0"
  },
  "dependencies": {
    "svg-injector": "1.1.3"
  },
  "directories": {
    "lib": "lib",
    "example": "example"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lifeiscontent/react-svg-injector.git"
  },
  "bugs": {
    "url": "https://github.com/lifeiscontent/react-svg-injector/issues"
  },
  "homepage": "https://github.com/lifeiscontent/react-svg-injector#readme",
  "jest": {
    "collectCoverage": true,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/example/"
    ],
    "setupFiles": [
      "raf/polyfill",
      "./jestSetup"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
